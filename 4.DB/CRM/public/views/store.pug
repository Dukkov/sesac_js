doctype html 
html(lang="ko")
    head 
        meta(charset="utf-8")
        title Store list 
        link(rel="stylesheet" type="text/css" href="/static/css/style.css")
    body
        section.nav-section
            include includes/nav.pug
        section.main-section
            table 
                thead 
                    tr 
                        th id
                        th type
                        th name
                        th address
                tbody 
                    for row in storeData 
                        tr 
                            td #{row.Id}
                            td #{row.Type}
                            td #{row.Name}
                            td #{row.Address}
        section.paging-section
            include includes/paging.pug