doctype html 
html(lang="ko")
    head 
        meta(charset="utf-8")
        title Item information
        link(rel="stylesheet" type="text/css" href="/static/css/style.css")
    body 
        section.nav-section
            include includes/nav.pug
        section.main-section
            p 상품 정보
            table
                thead 
                    tr 
                        th name
                        th unit_price
                tbody
                    tr 
                        td #{itemData.Name}
                        td #{itemData.UnitPrice}
            p 월간 매출액
            div.revenue-table-container
                table
                    thead
                        tr 
                            th Month
                            th Total Revenue
                            th Item Count 
                    tbody
                        for row in revenueData 
                            tr 
                                td #{row.Month}
                                td #{row.Revenue}
                                td #{row.ItemCnt}